{
  "parser": "babel-eslint",
  "extends": ["airbnb-base"],
  "rules": {
    "no-unused-vars": 0,
    "no-undef": "off",
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "react/button-has-type": 0,
    "jsx-a11y/label-has-for": 0
  },
  "env": {
    "jest": true,
    "browser": true
  }
}
